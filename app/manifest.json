{
  "name": "Github Marker",
  "version": "2.0",
  "description": "Link marker for Github",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://github.com/*",
        "*://gist.github.com/*"
      ],
      "css": [
        "styles.css"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "manifest_version": 2,
  "web_accessible_resources": [
    "assets/heart.v2.svg",
    "assets/heart-hollow.v2.svg",
    "assets/bookmark.v2.svg",
    "assets/bookmark-hollow.v2.svg",
    "assets/check.v2.svg",
    "assets/check-hollow.v2.svg"
  ],
  "icons": {
    "16": "icons/v2/icon16.png",
    "32": "icons/v2/icon32.png",
    "48": "icons/v2/icon48.png",
    "128": "icons/v2/icon128.png"
  }
}